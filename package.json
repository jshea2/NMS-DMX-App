{
  "name": "nms-dmx-app",
  "version": "1.0.0",
  "description": "Web-based DMX lighting control for faculty",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "dmx",
    "lighting",
    "sacn",
    "artnet"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "artnet": "^1.4.0",
    "cors": "^2.8.5",
    "e131": "^1.1.3",
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}